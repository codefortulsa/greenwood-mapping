    "version": "0.2.0",
    "configurations": [
        {
            "name": "Import data",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "import_data"
            ],
            "django": true,
            "justMyCode": true
        },
        {
            "name": "Django - runserver",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "8001"
            ],
            "django": true,
            "justMyCode": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Django - migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "migrate"
            ],
            "django": true,
            "justMyCode": false,
            "console": "integratedTerminal"
        },
    ]
}